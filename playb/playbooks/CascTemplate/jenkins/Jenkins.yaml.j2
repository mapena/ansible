########Openshift
  globalPluginConfiguration:
    buildConfigListInterval: 5
    buildListInterval: 5
    configMapListInterval: 5
    credentialsId: "sa-devops-cross-token-stg"
    enabled: true
    foldersEnabled: true
    imageStreamListInterval: 5
###    namespace: "almt-test-dev"
    namespace: {{ _namespace:}}
    secretListInterval: 5
    server: "{{ server }}"
######## FinOpenshift
######## Maven
  mavenModuleSet:
    localRepository: "default"
######## Fin maven
######## sonar
  sonarGlobalConfiguration:
    buildWrapperEnabled: false
    installations:
    - credentialsId: "sonar"
      name: "sonar"
      serverUrl: "http://10.0.47.30:9000"
      triggers:
        skipScmCause: false
        skipUpstreamCause: false
######## fin sonar
######## statistics-gatherer
  statisticsConfiguration:
    buildInfo: true
    buildStepInfo: false
    buildUrl: "https://deploy-tracking-almc-integrations-prd.prdcloud.bancogalicia.com.ar/"
    projectInfo: false
    queueInfo: false
    scmCheckoutInfo: false
    shouldPublishToAwsSnsQueue: false
    shouldSendApiHttpRequests: true
    shouldSendToLogback: false
  usageStatisticsCloudBees:
    usageStatisticsCollected: true
######## fin statistics-gatherer
