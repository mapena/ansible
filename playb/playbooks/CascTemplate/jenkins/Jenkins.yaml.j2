#######  CloudBees Jenkins Advisor  
advisor:
  acceptToS: true
  email: "tyt-alm@bancogalicia.com.ar"
  excludedComponents:
  - "GCLogs"
  - "Agents"
  - "AgentsConfigFile"
  - "ConfigFileComponent"
  - "RootCAs"
  - "SlaveLogs"
  - "OtherConfigFilesComponent"
  - "HeapUsageHistogram"
